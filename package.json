{
  "name": "gitbook-plugin-journal-summary",
  "version": "0.0.1",
  "description": "Plugin to generate gitbook's summary file for journal-style entries",
  "main": "index.js",
  "scripts": {
    "test": "mocha test/index.js"
  },
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "gitbook": {
    "properties": {
      "generateAll": {
        "type": "boolean",
        "default": false,
        "description": "Generate summary entries for Years and Month levels"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamell/gitbook-plugin-journal-summary.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gamell/gitbook-plugin-journal-summary/issues"
  },
  "homepage": "https://github.com/gamell/gitbook-plugin-journal-summary#readme",
  "dependencies": {
    "glob": "^7.1.2",
    "markdown-parser": "0.0.8",
    "moment": "^2.20.1",
    "tree-model": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.15.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^4.1.0",
    "sinon": "^4.1.3"
  }
}
